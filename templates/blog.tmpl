<div class="jubotron">
    <div class="container">
        <h1>Blog List</h1>
    </div>
    <table style="margin-bottom: 10px;">
        <tr>
            <th width="100px">blog_id</th>
            <th width="100px">title</th>
            <th width="100px">desc</th>
        </tr>
        {{range .Blogs}}
        <tr>
            <td><a href="./{{.BlogId}}/">{{.BlogId}}</a></td>
            <td>{{.Title}}</td>
            <td>{{.Description}}</td>
        </tr>
        {{end}}
    </table>
</div>